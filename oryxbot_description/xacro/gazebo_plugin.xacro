<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="oryxbot_gazebo_plugin">
  <xacro:macro name="oryxbot_gazebo_plugin">
    <gazebo>
      <plugin name="oryxbot_controller" filename="libgazebo_ros_planar_move.so">
        <commandTopic>cmd_vel</commandTopic>
        <odometryTopic>odom</odometryTopic>
        <odometryFrame>odom</odometryFrame>
        <odometryRate>20.0</odometryRate>
        <robotBaseFrame>base_footprint</robotBaseFrame>
      </plugin>
    </gazebo>
    <gazebo reference="base_footprint">
      <material>Gazebo/GreyTransparent</material>
      <mu1>0.0</mu1>
      <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="charge_port_link">
      <material>Gazebo/Orange</material>
    </gazebo>
    <gazebo reference="base_link">
      <material>Gazebo/White</material>
    </gazebo>
    <gazebo reference="base_cam_link">
      <material>Gazebo/FlatBlack</material>
    </gazebo>
    <gazebo reference="lf/wheel_link">
      <material>Gazebo/FlatBlack</material>
      <mu1>0.0</mu1>
      <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="lr/wheel_link">
      <material>Gazebo/FlatBlack</material>
      <mu1>0.0</mu1>
      <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="rf/wheel_link">
      <material>Gazebo/FlatBlack</material>
      <mu1>0.0</mu1>
      <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="rr/wheel_link">
      <material>Gazebo/FlatBlack</material>
      <mu1>0.0</mu1>
      <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="front_lidar_link">
      <material>Gazebo/BlueLaser</material>
    </gazebo>
    <gazebo reference="gaiban1_link">
      <material>Gazebo/FlatBlack</material>
    </gazebo>
    <gazebo reference="gaiban2_link">
      <material>Gazebo/RedGlow</material>
    </gazebo>
    <gazebo reference="rear_lidar_link">
      <material>Gazebo/BlueLaser</material>
    </gazebo>
    <gazebo reference="jiting_link">
      <material>Gazebo/RedGlow</material>
    </gazebo>
    <gazebo reference="box_1_link">
      <material>Gazebo/Blue</material>
    </gazebo>
    <gazebo reference="box_2_link">
      <material>Gazebo/Blue</material>
    </gazebo>
    <gazebo reference="guoxianhuan_link">
      <material>Gazebo/FlatBlack</material>
    </gazebo>
    <gazebo reference="control_box_link">
      <material>Gazebo/white</material>
    </gazebo>
    <gazebo reference="fuse_lidar_link">
      <sensor type="ray" name="rplidar_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>false</visualize>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>720</samples>
              <resolution>1</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>30.0</max>
            <resolution>0.01</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <!-- Noise parameters based on published spec for Hokuyo laser
                 achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
                 stddev of 0.01m will put 99.7% of samples within 0.03m of the true
                 reading. -->
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="oryxbot_fuse_lidar" filename="libgazebo_ros_laser.so">
          <topicName>/fuse_scan</topicName>
          <frameName>fuse_lidar_link</frameName>
        </plugin>
      </sensor>
    </gazebo>
    <gazebo reference="bottom_camera_link">
      <material>Gazebo/FlatBlack</material>
      <sensor type="camera" name="bottom_camera">
        <update_rate>30.0</update_rate>
        <camera name="bottom">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>720</width>
            <height>640</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <!-- Noise is sampled independently per pixel on each frame.
                 That pixel's noise value is added to each of its color
                 channels, which at that point lie in the range [0,1]. -->
            <mean>0.0</mean> 
            <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name="bottom_camera" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <updateRate>0.0</updateRate>
          <cameraName>/bottom_camera</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>bottom_camera_optical_link</frameName>
          <hackBaseline>0.07</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>
      </sensor>
    </gazebo>
    <gazebo reference="hand_cam_base_link">
      <material>Gazebo/FlatBlack</material>
      <sensor type="camera" name="hand_camera">
        <update_rate>30.0</update_rate>
        <camera name="hand_camera">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>720</width>
            <height>640</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <!-- Noise is sampled independently per pixel on each frame.
                 That pixel's noise value is added to each of its color
                 channels, which at that point lie in the range [0,1]. -->
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name="hand_camera" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <updateRate>0.0</updateRate>
          <cameraName>/hand_camera</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>hand_cam_link</frameName>
          <hackBaseline>0.07</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>
      </sensor>
    </gazebo>
  </xacro:macro>
</robot>
