<launch>
  <arg name="model" default="$(find xacro)/xacro --inorder '$(find oryxbot_description)/xacro/oryxbot.xacro'" />
  <arg name="gui" default="false" />
  <param name="robot_description" command="$(arg model)" />
  <!--CONTROL-->
<!-- Load joint controller configurations from YAML file to parameter server  -->
  <rosparam file="$(find oryxbot_description)/config/arm_controllers.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/swiftpro" args="joint_state_controller 
    joint1_position_controller 
    joint2_position_controller 
    joint3_position_controller 
    joint4_position_controller 
    joint5_position_controller 
    joint6_position_controller 
    joint7_position_controller  
    joint8_position_controller 
    joint9_position_controller"/>
  <!-- <node
    name="joint_state_publisher_gui"
    pkg="joint_state_publisher_gui"
    type="joint_state_publisher_gui" /> -->
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find oryxbot_description)/urdf.rviz" />
</launch>
